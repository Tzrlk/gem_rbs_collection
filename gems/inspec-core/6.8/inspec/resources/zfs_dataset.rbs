module Inspec::Resources
  class ZfsDataset
    def initialize: (untyped zfs_dataset) -> void

    # method called by 'it { should exist }'
    def exists?: () -> untyped

    def mounted?: () -> (false | untyped)

    def resource_id: () -> untyped

    def to_s: () -> ::String

    def gather: () -> (nil | untyped)

    # override method
